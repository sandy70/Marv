<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:libmarv="clr-namespace:LibMarv;assembly=LibMarv"
                    xmlns:libpipeline="clr-namespace:LibPipeline;assembly=LibPipeline"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DropShadowEffect x:Key="DropShadow" Opacity="0.3" ShadowDepth="3" />
    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <libmarv:BooleanNotConverter x:Key="BooleanNotConverter" />
    <libmarv:BooleanToColorConverter x:Key="BooleanToColorConverter" />
    <libpipeline:ILocationToLocationConverter x:Key="ILocationToLocation" />
    <libpipeline:IEnumerableILocationToIEnumerableLocationConverter x:Key="IEnumerableILocationToIEnumerableLocation" />
    <libmarv:BooleanToStringConverter x:Key="IsCheckedToImageSource" TrueString="/LibPipeline;component/Resources/Icons/antenna.png" FalseString="/LibPipeline;component/Resources/Icons/antenna-no-signal.png" />
    <libmarv:BooleanToStringConverter x:Key="IsHeaderToColor" TrueString="#b11d01" FalseString="#20aeff" />
    <libmarv:BooleanToStringConverter x:Key="IsLockedToImageSourceConverter" TrueString="/LibPipeline;component/Resources/Icons/lock.png" FalseString="/LibPipeline;component/Resources/Icons/unlock.png" />
