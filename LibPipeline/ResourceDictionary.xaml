    <libmarv:BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
    <libmarv:BooleanToVisibilityConverter x:Key="BooleanToVisibilityReversed" IsReversed="True" />
    <libmarv:DecimalToPercentageConverter x:Key="DecimalToPercentage" />
    <libmarv:DecimalPercentalConverter x:Key="DecimalPercentalConverter" />
    <libmarv:DoubleToJetGradientConverter x:Key="DoubleToJetColorConverter" />
    <libmarv:NullToVisibilityConverter x:Key="NullToVisibility" />
</ResourceDictionary>