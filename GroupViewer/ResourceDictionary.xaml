<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:libmarv="clr-namespace:LibMarv;assembly=LibMarv"
                    xmlns:properties="clr-namespace:GroupViewer.Properties"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DropShadowEffect x:Key="DropShadow" Opacity="0.3" ShadowDepth="3" />
    <libmarv:BooleanNotConverter x:Key="BooleanNot" />
    <libmarv:BooleanToColorConverter x:Key="BooleanToColorConverter"
                                         TrueColor="#b11d01"
                                         FalseColor="#DDDDDD" />
    <libmarv:BooleanToStringConverter x:Key="BooleanToStringConverter"
                                         TrueString="#b11d01"
                                         FalseString="#DDDDDD" />
    <libmarv:BooleanToStringConverter x:Key="ShowGroupsImageSource" TrueString="Resources/Icons/group.png" FalseString="Resources/Icons/ungroup.png" />
    <BooleanToVisibilityConverter x:Key="BooleanToVisibility" />
    <libmarv:DecimalPercentalConverter x:Key="DecimalPercentalConverter" />
    <libmarv:IEnumerableIntToStringConverter x:Key="IEnumerableIntToString" />
    <properties:Settings x:Key="Settings" />
</ResourceDictionary>